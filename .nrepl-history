(coll-contains? [1 2 3] 1)
(coll-contains? [1 2 3] 0)
(defn coll-contains? 
  "This exists because core contains? only works on maps."
  [coll x]
  (if (some #(= x %) coll) true false))
(coll-contains? [1 2 3] 0)
(coll-contains? [1 2 3] 1)
