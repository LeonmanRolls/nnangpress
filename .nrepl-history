(defn firebase-get
  "Get data from firebase database by route"  
  [route out] 
  (->
    (.ref (js/firebase.database) route)
    (.once "value")
    (.then (fn [snapshot]
             (put! 
               out 
               (mn/firebase-empty->clj-empty
                 (js->clj (.val snapshot) :keywordize-keys true)))))))
